{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravel/socialite": "^2.0",
        "anahkiasen/underscore-php": "^2.0",
        "cviebrock/eloquent-sluggable": "3.1.3",
        "graham-campbell/exceptions": "^8.2",
        "graham-campbell/flysystem": "^3.3",
        "guzzlehttp/guzzle": "6.1.1",
        "intervention/image": "^2.3",
        "ipunkt/laravel-analytics": "1.1.4",
        "jenssegers/rollbar": "^1.4",
        "jenssegers/date": "^3.1",
        "laracasts/presenter": "^0.2.1",
        "laravelbook/ardent": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-dropbox": "^1.0",
        "slynova/laravel-commentable": "^2.0",
        "spatie/laravel-glide": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.1",
        "barryvdh/laravel-debugbar": "^2.1",
        "filp/whoops": "^2.0",
        "flynsarmy/csv-seeder": "v1.0.6",
        "fzaninotto/faker": "~1.4",
        "hamcrest/hamcrest-php": "^1.2",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "sensiolabs/security-checker": "^3.0",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*",
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "vendor/bin/security-checker security:check",
            "php artisan clear-compiled",
	        "if php artisan env | grep local; then php artisan ide-helper:generate && php artisan ide-helper:meta; else echo 'Not generating IDE helpers outside local environment...'; fi;",
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "vendor/bin/security-checker security:check",
            "if php artisan env | grep local; then php artisan ide-helper:generate && php artisan ide-helper:meta; else echo 'Not generating IDE helpers outside local environment...'; fi;",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
